{
  "name": "SWDTeamBuilder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "refresh": "rm -rf node_modules && yarn cache clean && yarn install",
    "lint": "./node_modules/eslint/bin/eslint.js *.js ./src",
    "start": "react-native start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone SE'",
    "data": "yarn characters && yarn plots && yarn teams",
    "characters": "./node_modules/.bin/babel-node ./scripts/generate_characters.js",
    "plots": "./node_modules/.bin/babel-node ./scripts/generate_plots.js",
    "teams": "./node_modules/.bin/babel-node ./scripts/generate_teams.js"
  },
  "dependencies": {
    "bugsnag-react-native": "^2.2.3",
    "json-checksum": "^0.1.4",
    "jsonfile": "^5.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "16.5.0",
    "react-native": "0.57.2",
    "react-native-device-info": "^0.24.0",
    "react-native-safari-view": "^2.0.0",
    "react-native-swipeable": "^0.6.0",
    "react-native-swiper": "^1.5.5",
    "react-native-vector-icons": "^6.0.2",
    "react-navigation": "^2.0.1",
    "react-redux": "^5.0.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "swdestinydb-json-data": "https://github.com/rdonnelly/swdestinydb-json-data.git"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.3.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.1",
    "react-test-renderer": "16.5.2"
  },
  "resolutions": {
    "metro": "^0.48.1",
    "metro-babel-register": "^0.48.1",
    "metro-core": "^0.48.1",
    "metro-memory-fs": "^0.48.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
