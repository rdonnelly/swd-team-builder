{
  "name": "SWDTeamBuilder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone SE'",
    "ipad": "react-native run-ios --simulator='iPad Pro (10.5-inch)'",
    "start": "react-native start",
    "lint": "./node_modules/eslint/bin/eslint.js *.js ./src",
    "refresh": "rm -rf node_modules && yarn cache clean && yarn install",
    "test": "jest",
    "data": "yarn characters && yarn plots && yarn teams",
    "characters": "./node_modules/.bin/babel-node --presets @babel/preset-env ./scripts/generate_characters.js",
    "plots": "./node_modules/.bin/babel-node --presets @babel/preset-env ./scripts/generate_plots.js",
    "teams": "./node_modules/.bin/babel-node --presets @babel/preset-env --max-old-space-size=4096 ./scripts/generate_teams.js"
  },
  "dependencies": {
    "bugsnag-react-native": "2.13.x",
    "json-checksum": "0.1.x",
    "jsonfile": "5.0.x",
    "lodash": "4.17.x",
    "prop-types": "15.6.x",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-device-info": "0.24.x",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-haptic-feedback": "1.5.x",
    "react-native-safari-view": "2.1.x",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-native-swipeable": "0.6.x",
    "react-native-swiper": "1.5.x",
    "react-native-vector-icons": "6.1.x",
    "react-navigation": "3.0.x",
    "react-redux": "5.1.x",
    "redux": "4.0.x",
    "redux-logger": "3.0.x",
    "redux-thunk": "2.3.x",
    "reselect": "4.0.x",
    "swdestinydb-json-data": "https://github.com/rdonnelly/swdestinydb-json-data.git"
  },
  "devDependencies": {
    "@babel/node": "^7.2.x",
    "@babel/preset-env": "^7.2.x",
    "babel-eslint": "10.0.x",
    "babel-jest": "23.6.0",
    "eslint": "5.12.x",
    "eslint-config-airbnb-base": "13.1.x",
    "eslint-plugin-import": "2.14.x",
    "eslint-plugin-react": "7.12.x",
    "eslint-plugin-react-native": "3.6.x",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
