{
  "name": "SWDTeamBuilder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 11 Pro'",
    "ipad": "react-native run-ios --simulator='iPad Pro (10.5-inch)'",
    "se": "react-native run-ios --simulator='iPhone SE'",
    "start": "react-native start",
    "lint": "eslint .",
    "refresh": "rm -rf node_modules && yarn cache clean && yarn install",
    "test": "jest",
    "data": "yarn characters && yarn plots && yarn teams",
    "characters": "./node_modules/.bin/babel-node --presets @babel/preset-env ./scripts/generate_characters.js",
    "plots": "./node_modules/.bin/babel-node --presets @babel/preset-env ./scripts/generate_plots.js",
    "teams": "./node_modules/.bin/babel-node --presets @babel/preset-env --max-old-space-size=8192 ./scripts/generate_teams.js"
  },
  "dependencies": {
    "@react-native-community/slider": "^2.0.8",
    "bugsnag-react-native": "^2.23.3",
    "json-checksum": "0.1.x",
    "jsonfile": "5.0.x",
    "lodash": "^4.17.15",
    "prop-types": "15.7.x",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-device-info": "^5.4.0",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-haptic-feedback": "^1.8.2",
    "react-native-inappbrowser-reborn": "^3.3.3",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-sqlite-storage": "^4.1.0",
    "react-native-swipeable": "0.6.x",
    "react-native-swiper": "1.5.x",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "3.9.x",
    "react-redux": "5.1.x",
    "redux": "4.0.x",
    "redux-logger": "3.0.x",
    "redux-thunk": "2.3.x",
    "reselect": "4.0.x",
    "sqlite": "^3.0.3",
    "squel": "^5.13.0",
    "swdestinydb-json-data": "https://github.com/rdonnelly/swdestinydb-json-data.git"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.2.x",
    "@babel/preset-env": "^7.2.x",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "6.5.x",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
