{
  "name": "SWDTeamBuilder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 11 Pro'",
    "ipad": "react-native run-ios --simulator='iPad Pro (10.5-inch)'",
    "se": "react-native run-ios --simulator='iPhone SE'",
    "start": "react-native start",
    "lint": "eslint .",
    "refresh": "rm -rf node_modules && yarn cache clean && yarn install",
    "test": "jest --passWithNoTests",
    "data": "yarn data:characters && yarn data:plots && yarn data:teams",
    "data:characters": "babel-node ./scripts/generate_characters.js",
    "data:plots": "babel-node ./scripts/generate_plots.js",
    "data:teams": "babel-node --max-old-space-size=8192 ./scripts/generate_teams.js",
    "build:push": "scripts/build.sh push",
    "build:getVersion": "scripts/build.sh getVersion",
    "build:setVersion": "scripts/build.sh setVersion"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.5",
    "@react-native-community/slider": "2.0.x",
    "bugsnag-react-native": "2.23.x",
    "json-checksum": "0.1.x",
    "jsonfile": "5.0.x",
    "lodash": "4.17.x",
    "prop-types": "15.7.x",
    "react": "16.9.0",
    "react-native": "0.62.3",
    "react-native-device-info": "5.4.x",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-haptic-feedback": "1.8.x",
    "react-native-inappbrowser-reborn": "3.3.x",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-sqlite-storage": "4.1.x",
    "react-native-swipeable": "0.6.x",
    "react-native-swiper": "1.5.x",
    "react-native-vector-icons": "6.6.x",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^2.0.13",
    "react-navigation-tabs": "^2.7.0",
    "react-redux": "5.1.x",
    "redux": "4.0.x",
    "redux-logger": "3.0.x",
    "redux-thunk": "2.3.x",
    "reselect": "4.0.x",
    "sqlite": "3.0.x",
    "squel": "5.13.x",
    "swdestinydb-json-data": "https://github.com/rdonnelly/swdestinydb-json-data.git"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
